kf6-kdesu-devel_package() {
	depends="${sourcepkg}>=${version}_${revision} kf6-kpty-devel qt6-base-devel"
	short_desc+=" - development files"
	pkg_install() {
		vmove usr/include
		vmove usr/lib/cmake
		vmove "usr/lib/*.so"
	}
}
