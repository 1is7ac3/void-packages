kf6-frameworkintegration-devel_package() {
	depends="${sourcepkg}>=${version}_${revision}
	 kf6-kcolorscheme-devel kf6-kiconthemes-devel
	 kf6-kwidgetsaddons-devel"
	short_desc+=" - development files"
	pkg_install() {
		vmove usr/include
		vmove usr/lib/cmake
		vmove "usr/lib/*.so"
	}
}
